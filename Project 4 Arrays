#include<iostream>
using namespace std;
int main()
{
	int i,n;
	cout<<"How many numbers do you want to enter?\n";
	cin>>n;
	int arr[n];
	cout<<"Enter a set of numbers\n";
	for(i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	
	cout<<"The set of numbers you entered was: \n";
	for(i=0;i<n;i++)
	{
		cout<<arr[i]<<endl;
	}
}
